<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.finalproject2.SignupController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label fx:id="Signup" text="Signup"/>
    <padding>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
    </padding>
    <Label fx:id="Name" text="Login Name"/>
    <TextField fx:id="nameTextField" />
    <Label fx:id="Password" text="Password"/>
    <TextField fx:id="passwordTextField" />
    <Label fx:id="ConfirmPassword" text="Confirm Password"/>
    <TextField fx:id="confirmPasswordTextField" />
    <Button fx:id="submit" text="Submit" onAction="#onSubmit"/>
    <Label fx:id="errorLabel" />
</VBox>
